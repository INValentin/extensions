{
    "name": "Visualizer",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Visualize audio playing ia browser tab.",
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "tabCapture"
    ],
    "action": {
        "default_icon": {
            "128": "icon.png"
        },
        "default_popup": "popup.html"
    }
}